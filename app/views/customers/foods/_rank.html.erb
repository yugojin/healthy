<table class="table table-bordered border-dark">
  <thead><th class="rank text-center">いいねランキング</thead>
  <tbody>
    <td>
      <% foods.each do |food| %>
        <p><%=link_to food.food_name, customers_food_path(food.id) %></p>
      	  <i class="fa fa-heart" aria-hidden="true"></i>
      	<p><%= food.favorites.count %></p>
      <% end %>
    </td>
  </tbody>
</table>